{
    "welcomeFile": "index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/catalog/(.*)$",
            "authenticationType": "xsuaa",
            "destination": "srv"
        }
        ,
        {
            "source": "^/v2/catalog/(.*)$",
            "authenticationType": "xsuaa",
            "destination": "srv"
        }
        ,
        {
            "source": "^/(.*)$",
            "authenticationType": "xsuaa",
            "localDir": "resources/fiori"
        }
    ]
}